        <TextBlock x:Name="nonStrikerFour" HorizontalAlignment="Left" Margin="538,154,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="19" Height="16"/>
        <TextBlock x:Name="nonStrikerSix" HorizontalAlignment="Left" Margin="581,154,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="19" Height="16"/>
        <TextBlock x:Name="nonStrikerStrikeRate" HorizontalAlignment="Left" Margin="621,157,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="21" Height="16"/>
        <Label Content="Bowler" HorizontalAlignment="Left" Margin="24,177,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="bowlerName" HorizontalAlignment="Left" Margin="23,203,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="strikerRun" HorizontalAlignment="Left" Margin="452,130,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="21" Height="16"/>
        <TextBlock x:Name="bowlerMaiden" HorizontalAlignment="Left" Margin="495,203,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="19" Height="16"/>
        <TextBlock x:Name="bowlerRuns" HorizontalAlignment="Left" Margin="535,206,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="19" Height="16"/>
        <TextBlock x:Name="bowlerWicket" HorizontalAlignment="Left" Margin="578,206,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="19" Height="16"/>
        <TextBlock x:Name="bowlerEconomy" HorizontalAlignment="Left" Margin="618,203,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="19" Height="16"/>
