        <Label Content="Batsmen" HorizontalAlignment="Left" Margin="24,99,0,0" VerticalAlignment="Top"/>
        <Label Content="R" HorizontalAlignment="Left" Margin="454,99,0,0" VerticalAlignment="Top"/>
        <Label Content="B" HorizontalAlignment="Left" Margin="497,99,0,0" VerticalAlignment="Top"/>
        <Label Content="4s" HorizontalAlignment="Left" Margin="535,99,0,0" VerticalAlignment="Top"/>
        <Label Content="6s" HorizontalAlignment="Left" Margin="578,99,0,0" VerticalAlignment="Top"/>
        <Label Content="SR" HorizontalAlignment="Left" Margin="616,99,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="strikerName" HorizontalAlignment="Left" Margin="23,130,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="nonStrikerName" HorizontalAlignment="Left" Margin="23,151,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="bowlerOvers" HorizontalAlignment="Left" Margin="452,203,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="19" Height="16"/>
        <TextBlock x:Name="strikerBall" HorizontalAlignment="Left" Margin="495,130,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="23" Height="16"/>
        <TextBlock x:Name="strikerFour" HorizontalAlignment="Left" Margin="538,133,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="19" Height="16"/>
        <TextBlock x:Name="strikerSix" HorizontalAlignment="Left" Margin="578,133,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="19" Height="16"/>
        <TextBlock x:Name="strikerStrikeRate" HorizontalAlignment="Left" Margin="621,133,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="21" Height="16"/>
        <TextBlock x:Name="nonStrikerRun" HorizontalAlignment="Left" Margin="452,154,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.265,0.156" Width="21" Height="16"/>
