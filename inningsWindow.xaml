        <TextBlock x:Name="displayOver_Copy1" HorizontalAlignment="Left" Margin="106,41,0,0" TextWrapping="Wrap" Text="." VerticalAlignment="Top"/>
        <Button x:Name="zero" Content="0" HorizontalAlignment="Left" Margin="350,295,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button x:Name="one" Content="1" HorizontalAlignment="Left" Margin="452,295,0,0" VerticalAlignment="Top" Width="75" Click="one_Click"/>
        <Button x:Name="two" Content="2" HorizontalAlignment="Left" Margin="553,295,0,0" VerticalAlignment="Top" Width="75" Click="two_Click"/>
        <Button x:Name="three" Content="3" HorizontalAlignment="Left" Margin="650,295,0,0" VerticalAlignment="Top" Width="75" Click="three_Click"/>
        <Button x:Name="four" Content="4" HorizontalAlignment="Left" Margin="350,321,0,0" VerticalAlignment="Top" Width="75" Click="four_Click"/>
        <Button x:Name="five" Content="5" HorizontalAlignment="Left" Margin="454,321,0,0" VerticalAlignment="Top" Width="75" Click="five_Click"/>
        <Button x:Name="six" Content="6" HorizontalAlignment="Left" Margin="553,321,0,0" VerticalAlignment="Top" Width="75" Click="six_Click"/>
        <Label Content="Overs" HorizontalAlignment="Left" Margin="83,10,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="displayWicket" HorizontalAlignment="Left" Margin="42,41,0,0" TextWrapping="Wrap" Text="0 " VerticalAlignment="Top"/>
        <TextBlock x:Name="displayScore_Copy" HorizontalAlignment="Left" Margin="34,41,0,0" TextWrapping="Wrap" Text="/" VerticalAlignment="Top"/>
        <Label Content="Enter New Batsman Name" HorizontalAlignment="Left" Margin="325,355,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="newBatsmanName" HorizontalAlignment="Left" Height="23" Margin="340,381,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Content="Continue" HorizontalAlignment="Left" Margin="465,381,0,0" VerticalAlignment="Top" Width="49" Height="23" Click="Button_Click_1"/>
        <CheckBox x:Name="runOutStriker" Content="runout striker" HorizontalAlignment="Left" Margin="37,363,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="runOutNonStriker" Content="runout non-striker" HorizontalAlignment="Left" Margin="36,383,0,0" VerticalAlignment="Top"/>
        <Label Content="Enter Next Bowler" HorizontalAlignment="Left" Margin="553,357,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="newBowler" HorizontalAlignment="Left" Height="23" Margin="563,381,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Content="Continue" HorizontalAlignment="Left" Margin="698,384,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <Button x:Name="swapBatsman" Content="SwapBatsman" HorizontalAlignment="Left" Margin="650,321,0,0" VerticalAlignment="Top" Width="75" Click="swapBatsman_Click"/>
        <Rectangle Fill="#FF6FB265" HorizontalAlignment="Left" Height="79" Margin="15,98,0,0" Stroke="Black" VerticalAlignment="Top" Width="652" Opacity="0.5"/>

    </Grid>
</Window>
