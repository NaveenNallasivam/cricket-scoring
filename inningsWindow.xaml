<Window x:Class="player_profile.inningsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:player_profile"
        mc:Ignorable="d"
        Title="inningsWindow" Height="450" Width="800">
    <Grid x:Name="newBatsmanButton" Margin="-2,0,2,0">
        <Label Content="Score" HorizontalAlignment="Left" Margin="23,10,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="displayScore" HorizontalAlignment="Left" Margin="23,41,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top"/>
