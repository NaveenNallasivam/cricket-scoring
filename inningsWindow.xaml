        <Label Content="m" HorizontalAlignment="Left" Margin="493,177,0,0" VerticalAlignment="Top" FontFamily="Segoe UI Semibold"/>
        <Label Content="r" HorizontalAlignment="Left" Margin="535,178,0,0" VerticalAlignment="Top" FontFamily="Segoe UI Semibold"/>
        <Label Content="w" HorizontalAlignment="Left" Margin="578,180,0,0" VerticalAlignment="Top" FontFamily="Segoe UI Semibold"/>
        <Label Content="er" HorizontalAlignment="Left" Margin="616,175,0,0" VerticalAlignment="Top" FontFamily="Segoe UI Semibold"/>
        <Rectangle Fill="#FF65CD2A" HorizontalAlignment="Left" Height="150" Margin="24,251,0,0" Stroke="Black" VerticalAlignment="Top" Width="128"/>
        <CheckBox x:Name="wide" Content="Wide" HorizontalAlignment="Left" Margin="37,264,0,0" VerticalAlignment="Top"/>
